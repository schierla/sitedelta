{
	"name": "SiteDelta",
	"version": "0.14.3",
	"manifest_version": 2,
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions": [
		"activeTab"
	],
	"browser_action": {
		"browser_style": true,
		"default_icon": {
			"16": "icons/icon-16.png",
			"32": "icons/icon-32.png",
			"64": "icons/icon-64.png"
		},
		"default_title": "SiteDelta",
		"default_popup": "popup.htm"
	},
	"applications": {
		"gecko": {
			"id": "sitedelta@schierla.de"
		}
	}
}